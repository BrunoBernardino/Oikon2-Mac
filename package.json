{
  "name": "Oikon2-Mac",
  "version": "1.0.0",
  "description": "Manage Your Expenses",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "prebuild": "npm install && npm run lint && npm test",
    "postinstall": "./node_modules/.bin/electron-rebuild",
    "build": "rm -fr build && node ./build.js",
    "release": "npm run build -- --sign && node_modules/.bin/electron-osx-flat \"build/Oikon 2-mas-x64/Oikon 2.app\" --verbose",
    "lint": "node_modules/.bin/eslint *.js components/** utils/**",
    "test": "./node_modules/mocha/bin/mocha --timeout=5000",
    "ci-test": "./node_modules/mocha/bin/mocha --timeout=25000"
  },
  "repository": "https://github.com/BrunoBernardino/Oikon2-Mac",
  "keywords": [
    "expenses",
    "simple",
    "minimal",
    "oikon",
    "mac",
    "ios"
  ],
  "author": "BrunoBernardino",
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "electron": "1.6.8",
    "electron-packager": "8.7.0",
    "electron-rebuild": "1.5.11",
    "eslint": "3.19.0",
    "eslint-plugin-react": "7.0.1",
    "mocha": "3.4.1",
    "spectron": "3.6.4"
  },
  "dependencies": {
    "babel-preset-react": "6.24.1",
    "babel-register": "6.24.1",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "notie": "4.3.1",
    "pouchdb": "6.2.0",
    "pouchdb-find": "6.2.0",
    "prop-types": "15.5.10",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "sweetalert": "1.1.3",
    "switchery": "0.0.2"
  }
}
